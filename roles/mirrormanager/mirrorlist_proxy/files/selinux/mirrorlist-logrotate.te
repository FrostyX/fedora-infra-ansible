module mirrorlist-logrotate 1.0;

require {
	type logrotate_t;
	type svirt_sandbox_file_t;
	class file { setattr create write };
	class dir { write add_name remove_name };
}

#============= logrotate_t ==============
allow logrotate_t svirt_sandbox_file_t:dir { add_name remove_name write };
